{
  "name": "racker",
  "version": "1.0.0",
  "description": "Rack diagram planning tool with power and HVAC capacity tracking",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "cd backend && python manage.py runserver 3000",
    "start": "npm run build && npm run server",
    "db:init": "cd backend && python manage.py migrate",
    "docs:serve": "mkdocs serve",
    "docs:build": "mkdocs build",
    "docs:deploy": "mkdocs gh-deploy",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "rack",
    "datacenter",
    "planning",
    "diagram"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vueuse/core": "^14.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "tailwindcss": "^4.1.17",
    "vite": "^7.2.2",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@vitest/ui": "^2.1.0",
    "@vue/test-utils": "^2.4.6",
    "daisyui": "^5.4.7",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^25.0.0",
    "prettier": "^3.3.0",
    "vitest": "^2.1.0"
  }
}
